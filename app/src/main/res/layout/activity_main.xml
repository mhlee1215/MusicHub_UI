<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#232532"
    android:gravity="center_horizontal"
    >

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            >

            <View android:layout_width="180dp"
                android:layout_height="180dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/circle_1"
                />

            <View android:layout_width="102dp"
                android:layout_height="102dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/circle_2"/>



            <View android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/circle_3"
                />

            <View android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/circle_4"

                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="55dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            >

            <LinearLayout
                android:id="@+id/turnUpText"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"

                android:gravity="center"
                >

                <TextView
                    style="@style/text_view_8FAE750659B74A5A9923F4721DA38941_style"
                    android:id="@+id/text_view_8FAE750659B74A5A9923F4721DA38941"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TURN IT UP"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="50dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerHorizontal="true"
                >

                <View
                    android:layout_marginRight="10dp"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:background="@drawable/circle_5"
                    />

                <View
                    android:layout_marginRight="10dp"
                    android:layout_width="6dp"
                    android:layout_height="6dp"


                    android:background="@drawable/circle_5"
                    />

                <View
                    android:layout_marginRight="10dp"
                    android:layout_width="6dp"
                    android:layout_height="6dp"


                    android:background="@drawable/circle_5"
                    />

                <View
                    android:layout_marginRight="10dp"
                    android:layout_width="6dp"
                    android:layout_height="6dp"


                    android:background="@drawable/circle_5"
                    />

                <View

                    android:layout_width="6dp"
                    android:layout_height="6dp"


                    android:background="@drawable/circle_6"
                    />
            </LinearLayout>

        </RelativeLayout>


        <RelativeLayout
            android:layout_marginTop="55dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            >

            <Button
                android:layout_width="242dp"
                android:layout_height="42dp"
                android:background="@drawable/button_1"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:gravity="center"
                >
            </Button>

            <TextView
                style="@style/text_view_5390BC46DAB9400B81B896DB1CF901E2_style"
                android:id="@+id/text_view_5390BC46DAB9400B81B896DB1CF901E2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="FIND A CHANNEL"
                />





        </RelativeLayout>
    </LinearLayout>




</RelativeLayout>
